<property> propulsion/engine/blade_damage</property>
  <channel name="Engine">
   <fcs_function name="fcs/propeller-pitch">
     <function>
      <sum>
       <product>
        <property>fcs/advance-pos-norm</property>
        <value>-25</value>
       </product>
       <value>47.5</value>
       <product>
         <property> propulsion/engine/blade_damage</property>
        <value>50</value>
       </product>
      </sum>
     </function>
     <output>propulsion/engine/blade-angle</output>
  </fcs_function>

