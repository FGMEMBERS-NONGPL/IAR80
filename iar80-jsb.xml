        <property>propulsion/engine/oil-burn-pps</property>
     </function>
     <output> propulsion/tank[2]/external-flow-rate-pps </output>
  </fcs_function>

   <fcs_function name="propulsion/functions/fuel-press-norm">
     <function>
       <table>
         <independentVar lookup="row">propulsion/tank[1]/contents-lbs</independentVar>
         <tableData>
            2	0.65
           30	1.
         </tableData>
        </table>
    </function>
   </fcs_function>

   <fcs_function name="propulsion/functions/mixture-ctrl">
